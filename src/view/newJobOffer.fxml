<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="210.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.NewJobOfferController">
   <children>
      <HBox alignment="CENTER" layoutX="5.0" layoutY="5.0" prefHeight="61.0" prefWidth="400.0">
         <children>
            <Label contentDisplay="CENTER" prefHeight="49.0" prefWidth="250.0" text="Enter your offer for the job below:">
               <font>
                  <Font size="16.0" />
               </font>
            </Label>
         </children>
      </HBox>
      <HBox alignment="CENTER" layoutX="5.0" layoutY="66.0" prefHeight="61.0" prefWidth="400.0">
         <children>
            <TextField fx:id="OfferField" />
         </children>
      </HBox>
      <HBox alignment="CENTER" layoutX="10.0" layoutY="130.0" prefHeight="42.0" prefWidth="394.0" spacing="10.0">
         <children>
            <Button mnemonicParsing="false" onAction="#okButtonHandle" text="OK" />
            <Button mnemonicParsing="false" onAction="#cancelButtonHandle" text="Cancel" />
         </children>
      </HBox>
      <HBox alignment="CENTER" layoutY="172.0" prefHeight="42.0" prefWidth="409.0" spacing="10.0">
         <children>
            <Label fx:id="messageLabel" />
         </children>
      </HBox>
   </children>
</AnchorPane>
